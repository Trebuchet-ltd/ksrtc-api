{
  "kind": "collectionType",
  "collectionName": "trips",
  "info": {
    "name": "Trip"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "start_time": {
      "type": "datetime"
    },
    "end_time": {
      "type": "datetime"
    },
    "bus": {
      "model": "bus",
      "via": "trips"
    },
    "type": {
      "type": "string"
    },
    "conductor": {
      "via": "trips",
      "plugin": "users-permissions",
      "columnName": "Conductor",
      "model": "user"
    },
    "lat": {
      "type": "float"
    },
    "long": {
      "type": "float"
    },
    "route": {
      "model": "route"
    },
    "next_stop": {
      "model": "hub"
    },
    "from": {
      "type": "string"
    },
    "to": {
      "type": "string"
    },
    "hub": {
      "model": "hub",
      "via": "trips"
    },
    "number_of_passengers": {
      "type": "integer"
    },
    "total_collection": {
      "type": "integer"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "not_started",
        "in_progress",
        "completed"
      ],
      "default": "not_started"
    },
    "driver": {
      "via": "trips_driver",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
